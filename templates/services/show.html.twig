{% extends 'base.html.twig' %}

{% block title %}Service{% endblock %}

{% block body %}
    <h1>Service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service.id }}</td>
            </tr>
            <tr>
                <th>NameS</th>
                <td>{{ service.nameS }}</td>
            </tr>
            <tr>
                <th>DescriptionS</th>
                <td>{{ service.descriptionS }}</td>
            </tr>
            <tr>
                <th>AvailabilityDate</th>
                <td>{{ service.availabilityDate ? service.availabilityDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Localisation</th>
                <td>{{ service.localisation }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ service.state }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_services_index') }}">back to list</a>

    <a href="{{ path('app_services_edit', {'id': service.id}) }}">edit</a>

    {{ include('services/_delete_form.html.twig') }}
{% endblock %}
