        {% if table is empty %}
        <div class="text-center"> <!-- Center the content -->
            <p class="text-danger" style="font-size: 24px;">
                <i class="fas fa-shopping-cart"></i> Your orders list is empty!
            </p>
        </div>
        
        {% else %}
        <div class="table-responsive">
            <table class="table  datanew">
                <thead>
                    <tr>
                        
                        <th>Commande ID</th>
                        <th>Date</th>
                        <th>City</th>
                        <th>Delivery Methode</th>
                        <th>Product Name</th>
                        <th></th>
                        <th></th>
                  
                    </tr>
                </thead>
                <tbody>
                    {% for j in table %}
                    <tr>
                       
                       
                        <td>{{j.getId()}}</td>
                        <td>{{j.getDateCmd()|date('Y-m-d H:i:s') }}</td>
                        <td>{{j.getVille()}}</td>
                        <td>{{j.getMethodeLivraison()}} </td>
                       
                        <td>{{ j.getProduit().getNomProduit() }}</td>
                        <td>
                            <a class="btn btn-sm btn-danger rounded py-2 px-4" href="{{ path('deletecFE', {'id': j.id}) }}"> <i class="fas fa-trash-alt me-1"></i>Cancel Order</a>
                        </td>
                       
                    </tr>

                   
                    {% endfor %}
                </tbody>
            </table>
        </div>
        {% endif %}


        






